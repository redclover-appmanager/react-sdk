{
  "name": "@redclover/koru-react-sdk",
  "version": "1.0.0",
  "description": "Lightweight React SDK for Koru platform authorization and integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-redclover:redclover-appmanager/react-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/redclover-appmanager/react-sdk/issues"
  },
  "homepage": "https://github.com/redclover-appmanager/react-sdk#readme",
  "keywords": [
    "react",
    "sdk",
    "koru",
    "typescript",
    "authorization",
    "hooks",
    "provider",
    "integration"
  ],
  "author": {
    "name": "Red Clover",
    "email": "soporte@redclover.com.ar"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "sideEffects": false
}
